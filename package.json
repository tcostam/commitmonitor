{
  "name": "commitmonitor-frontend",
  "version": "0.0.1",
  "private": true,
  "description": "commitmonitor frontend assets.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "start": "babel-node server.js",
    "build": "NODE_ENV=production webpack -p --progress --colors --config webpack.prod.config.js --bail",
    "lint": "eslint assets",
    "coverage": "jest --coverage"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/jest-css-modules"
    },
    "collectCoverageFrom": [
      "assets/js/**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "assets/js/store.js",
      "assets/js/index.js",
      "assets/js/jquery-index.js",
      "assets/js/constants/*",
      "assets/js/pages/*",
      "assets/js/tests/*"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 10
      }
    },
    "modulePaths": [
      "assets",
      "assets/js",
      "assets/js/app"
    ]
  },
  "dependencies": {
    "autoprefixer": "^7.2.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bootstrap": "^4.1.1",
    "bootstrap-loader": "^2.2.0",
    "classnames": "^2.2.6",
    "css-loader": "^0.28.11",
    "expose-loader": "^0.7.5",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.11.2",
    "imports-loader": "^0.7.1",
    "jquery": "^3.3.1",
    "lodash": "^4.17.10",
    "marked": "^0.3.19",
    "node-sass": "^4.9.0",
    "popper.js": "^1.14.3",
    "postcss": "^6.0.22",
    "postcss-loader": "^2.1.5",
    "prop-types": "^15.6.2",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "redux": "^3.7.2",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^6.0.7",
    "script-loader": "^0.7.2",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.12.0",
    "webpack-bundle-tracker": "^0.2.1",
    "webpack-dev-server": "^2.11.2",
    "webpack-spritesmith": "^0.3.6",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "ajv": "^6.5.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.9.1",
    "jest": "^21.2.1",
    "jest-css-modules": "^1.1.0",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "^15.6.2"
  }
}
